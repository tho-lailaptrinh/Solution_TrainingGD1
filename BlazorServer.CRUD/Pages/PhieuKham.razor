@page "/phieukham"
<h3>PhieuKham</h3>
@if (ListPhieuKham == null)
{
    <td>loading</td>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th>Tên Phiếu Khám</th>
                <th>Tên Khách Hàng</th>
                <th>Tên Bác Sĩ</th>
                <th>Tên Dịch Vụ</th>
                <th>Tên Phòng</th>
                <th>Thời Gian Tạo</th>
                <th>Trạng Thái</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ListPhieuKham)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.NameKH</td>
                    <td>@item.NameNV</td>
                    <td>@item.NameDV</td>
                    <td>@item.NamePK</td>
                    <td>@item.CreateDate.ToString("mm/dd/yyyy hh/mm")</td>
                    <td>@item.Status</td>
                </tr>
            }
        </tbody>
    </table>
}

